{
    "name": "symplify/package-builder",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "nette/bootstrap": "^2.4|^3.0",
        "nette/di": "^2.4|^3.0",
        "nette/neon": "^2.4|^3.0",
        "nette/utils": "^2.4|^3.0",
        "symfony/config": "^3.3",
        "symfony/console": "^3.3",
        "symfony/dependency-injection": "^3.3",
        "symfony/http-kernel": "^3.3",
        "symfony/yaml": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "tracy/tracy": "^2.4"
